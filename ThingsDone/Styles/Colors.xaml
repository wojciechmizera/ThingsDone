<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ThingsDone">


    <Color x:Key="OuterColor">#FFFAEBD7</Color>
    <SolidColorBrush x:Key="OuterBrush" Color="{StaticResource OuterColor}"/>

    <Color x:Key="BackgroundColor">#FFF0F8FF</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>

    <Color x:Key="BorderColor">#FF9ACD32</Color>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderColor}"/>

    <Color x:Key="LightGrayColor">#FFB4B4B4</Color>
    <SolidColorBrush x:Key="LightGrayBrush" Color="{StaticResource LightGrayColor}"/>
</ResourceDictionary>